@charset "gb2312";
/* CSS Document */

* { margin:0; padding:0}
body{ font:12px "Î¢ÈíÑÅºÚ", "ËÎÌå"; background-image:url(../images/xiegang_bg.jpg);}
.clear {clear:both;}
ol,ul { list-style:none;}
img {border:none;}
a {outline:none; text-decoration:none; color:#333333}
table { border-collapse:collapse; border-spacing:0;}
#box,#nav{
  -moz-border-radius:2px; /* Firefox */
  -webkit-border-radius:2px; /* Safari and Chrome */
  border-radius:2px; /* Opera 10.5+, future browsers, and now also Internet Explorer 6+ using IE-CSS3 */
 
  -moz-box-shadow:0px 1px 3px rgba(0, 0, 0, 0.2); /* Firefox */
  -webkit-box-shadow:0px 1px 3px rgba(0, 0, 0, 0.2); /* Safari and Chrome */
  box-shadow:0px 1px 3px rgba(0, 0, 0, 0.2); /* Opera 10.5+, future browsers and IE6+ using IE-CSS3 */
 
  /* behavior: url(http://building-dreams.com/mywork/ie-css3.htc); This lets IE know to call the script on all elements which get the 'box' class */
  background-color:white;
}
